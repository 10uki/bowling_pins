package bowling_pins;

public class bowling_Pins {

	public static void print_Pins(int n, int j) {
		int i;
		if (n == 0)
			return;
		for (i = 0; i < n - j; i++)
			System.out.print(" ");
		for (i = 0; i < j; i++)
			System.out.print("* ");
		System.out.println();
		if (j < n) {
			print_Pins(n, j + 1);
		}
	}
}
